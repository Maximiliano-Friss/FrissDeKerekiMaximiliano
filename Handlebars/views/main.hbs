    <h2>Chat</h2>
    <form id="form2">
        <div class="form-group">
            <input placeholder="Ingresar email" id="email" class="form-control" type="email" name="email">
        </div>
        <div class="form-group">
            <input placeholder="Ingresar nombre" id="nombre" class="form-control" type="text" name="nombre">
